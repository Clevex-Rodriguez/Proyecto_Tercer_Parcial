<PantallaAdministrativos>:
    name: 'pantallaAdministrativos'
    id: Administrativos
    BoxLayout:
        orientation: 'vertical'
        size_hint: (None, None)
        size: (500, 400)
        pos: (250, 100)
        canvas:
            Color:
                rgba: [0, 0, 0, 1]
            Rectangle:
                pos: self.pos
                size: self.size
        Label:
            text: 'BIBLIOTECA CENTRAL- UNT'
            font_size: 60
            color: [0, 0, 1, .5]
            bold: True
            italic: True
        GridLayout:
            cols: 2
            Label:
            Label:
            Label:
                text: 'Contraseña'
                font_size: 20
                bold: True
            TextInput:
                id: contr
                font_size: 20
                multiline: False
                password: True
                hint_text: 'Contraseña'
            Label:
            Label:  
        FloatLayout:
            size_hint: (None, None)
            size: (100, 50)
            #pos: (250, 100)
            Button:
                id: validar
                text: 'Validar'
                font_size: 20
                pos: (450, 100)
                #background_color: '#162969'
                #on_release: Factory.Contras_vacia().open()
                on_release: root.contr_avisos(*args)
            Button:
                text: 'Atrás'
                font_size: 20
                pos: (100, 50)
                on_release: app.root.current = 'estudiantes_administrativos'
            Label:
                text: 'Ver contraseña'
                pos: (755, 215)
            CheckBox:
                pos: (820, 215)
                on_active: root.checkbox_click(self, self.active)

<Contras_vacia>:
